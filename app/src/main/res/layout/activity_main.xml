<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scrollView1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" >
    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#C6C6C8"
        tools:context=".MainActivity">

        <ImageButton
            android:id="@+id/imagen1"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:layout_row="0" android:layout_column="0"
            android:src="@drawable/ferianavidad" />

        <ImageButton
            android:src="@drawable/jalisco"
            android:layout_row="0" android:layout_column="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imagen2"
            />

        <ImageButton
            android:src="@drawable/familia"
            android:layout_row="0" android:layout_column="2"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:id="@+id/imagen3"
            />

        <ImageButton
            android:src="@drawable/bodascar"
            android:layout_row="1" android:layout_column="0"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:id="@+id/imagen4"
            />

        <ImageButton
            android:id="@+id/imagen5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="1"
            android:src="@drawable/cumplecristo" />

        <ImageButton
            android:src="@drawable/laguna"
            android:layout_row="1" android:layout_column="2"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:id="@+id/imagen6"
            />

        <ImageButton
            android:src="@drawable/dianuevo"
            android:layout_row="2" android:layout_column="0"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:id="@+id/imagen7"
            />

        <ImageButton
            android:src="@drawable/mazamitla"
            android:layout_row="2" android:layout_column="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imagen8"
            />

        <ImageButton
            android:src="@drawable/bautizo"
            android:layout_row="2" android:layout_column="2"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:id="@+id/imagen9"
            />

        <ImageButton
            android:src="@drawable/campafam"
            android:layout_row="3" android:layout_column="0"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:id="@+id/imagen10"
            />

        <ImageButton
            android:src="@drawable/futbol1"
            android:layout_row="3" android:layout_column="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imagen11"
            />

        <ImageButton
            android:src="@drawable/posada"
            android:layout_row="3" android:layout_column="2"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:id="@+id/imagen12"
            />

        <ImageButton
            android:src="@drawable/cumpleabue"
            android:layout_row="4" android:layout_column="0"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:id="@+id/imagen13"
            />

        <ImageButton
            android:src="@drawable/primos"
            android:layout_row="4" android:layout_column="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imagen14"
            />

        <ImageButton
            android:src="@drawable/fiesta"
            android:layout_row="4" android:layout_column="2"
            android:layout_width="127dp"
            android:layout_height="wrap_content"
            android:id="@+id/imagen15"
            />

    </GridLayout>
</ScrollView>